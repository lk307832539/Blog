<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.entity.Channel" table="channel" schema="blog">
        <id name="channelId" column="channel_id"/>
        <property name="channelName" column="channel_name"/>
        <property name="parentId" column="parent_id"/>
        <property name="isDisplay" column="is_display"/>
        <property name="titleImg" column="title_img"/>
        <property name="description" column="description"/>
    </class>
</hibernate-mapping>